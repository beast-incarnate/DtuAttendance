<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kunalsingh.dtuattendance">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity"></activity>
        <activity android:name=".activities.MarkingAttendaceTeacher" />
        <activity android:name=".activities.AdminActivity" />
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.LoginOptionsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.ParentActivity" />
        <activity android:name=".activities.StudentActivity" />
        <activity android:name=".activities.TeacherActivity" />
        <activity android:name=".activities.ViewTimeTableActivity"
            android:screenOrientation="landscape"></activity>
    </application>

</manifest>